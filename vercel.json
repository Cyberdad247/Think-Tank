{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/apps/frontend/$1"
    }
  ],
  "projects": [
    {
      "name": "think-tank-frontend",
      "root": "apps/frontend",
      "buildCommand": "npm run build",
      "outputDirectory": ".next"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, max-age=0"
        }
      ]
    }
  ],
